

{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "vpc via CF",
    "Resources": {
        "cfvpc": {
            "Type": "AWS::EC2::VPC",
            "Properties": {
                "CidrBlock": "192.168.0.0/16",
                "Tags": [
                    {
                        "Key": "cfvpc",
                        "Value": "datavpc"
                    }
                ]
            }
        },
        "websubnet": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "AvailabilityZone": "us-west-2b",
                "VpcId": {
                    "Ref": "cfvpc"
                },
                "CidrBlock": "192.168.0.0/24",
                "Tags": [
                    {
                        "Key": "cfsubnet1",
                        "Value": "websubnet"
                    }
                ]
            }
        },
        "datasubnet": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "AvailabilityZone": "us-west-2a",
                "VpcId": {
                    "Ref": "cfvpc"
                },
                "CidrBlock": "192.168.1.0/24",
                "Tags": [
                    {
                        "Key": "cfsubnet2",
                        "Value": "datasubnet"
                    }
                ]
            }
        },
        "cfigw": {
            "Type": "AWS::EC2::InternetGateway",
            "Properties": {
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": "cfigw"
                    }
                ]
            }
        },
        "AttachGateway": {
            "Type": "AWS::EC2::VPCGatewayAttachment",
            "Properties": {
                "VpcId": {
                    "Ref": "cfvpc"
                },
                "InternetGatewayId": {
                    "Ref": "cfigw"
                }
            }
        },
        "RouteTable1": {
            "Type": "AWS::EC2::RouteTable",
            "Properties": {
                "VpcId": {
                    "Ref": "cfvpc"
                },
                "Tags": [
                    {
                        "Key": "RouteTable1",
                        "Value": "route"
                    }
                ]
            }
        },
        "forwardToIGW": {
            "Type": "AWS::EC2::Route",
            "Properties": {
                "DestinationCidrBlock": "0.0.0.0/0",
                "RouteTableId": {
                    "Ref": "RouteTable1"
                },
                "GatewayId": {
                    "Ref": "cfigw"
                }
            }
        },
        "webrouteassociation": {
            "Description": "Associating websubnet",
            "Type": "AWS::EC2::SubnetRouteTableAssociation",
            "Properties": {
                "RouteTableId": {
                    "Ref": "RouteTable1"
                },
                "SubnetId": {
                    "Ref": "websubnet"
                }
            }
        },
        "datarouteassociation": {
            "Description": "Associating websubnet",
            "Type": "AWS::EC2::SubnetRouteTableAssociation",
            "Properties": {
                "RouteTableId": {
                    "Ref": "RouteTable1"
                },
                "SubnetId": {
                    "Ref": "datasubnet"
                }
            }
        }
    }
}